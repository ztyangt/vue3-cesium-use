{
  "name": "vue3-cesium-use-root",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "dictory": "packages"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type:check \"build:only {@}\" --",
    "release": "esno ./scripts/release.ts",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build:only": "rm -r packages/dist && vite build",
    "type:check": "vue-tsc --noEmit --allowImportingTsExtensions && tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev docs --port 8000 --host 0.0.0.0",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 9000"
  },
  "dependencies": {
    "cesium": "^1.121.1",
    "vue": "^3.5.6",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "consola": "^3.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "esno": "^4.7.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.78.0",
    "sass-embedded": "^1.78.0",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.18.3",
    "vite": "^5.3.1",
    "vite-plugin-cesium": "^1.2.23",
    "vite-plugin-dts": "^4.2.1",
    "vitepress": "^1.3.4",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.24.2"
  }
}
